#include<stdio.h>
void Bubble_sort(int a[],int size)
{
  int i,j,k;
  int count=0;
  for(i=0;i<size-1;i++)
  {
      for(j=0;j<size-1-i;j++)
      {
          if(a[j]>a[j+1])
            {
                k=a[j+1];
                a[j+1]=a[j];
                a[j]=k;
                count=1;
            }
        if(count==0)
          {
              break;
          }
      }
  ]
